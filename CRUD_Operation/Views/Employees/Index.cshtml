@model List<CRUD_Operation.Models.Employee>
 
	<h1> Employees</h1>

	<table class="table">
		<thead>
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th>Address</th>
				<th>Mobile</th>
				<th>Date Of Birth</th>

			</tr>
		</thead>
		<tbody>
			@foreach(var employee in Model)
			{
				<tr>
					<td>@employee.Id</td>
					<td>@employee.Name</td>
					<td>@employee.Address</td>
					<td>@employee.DateOfBirth</td>
					<td><a href ="Employees/View/@employee.Id">View</a></td>

				</tr>
			}
		</tbody>
		</table>
<a href="Employees/Add">Add</button>

